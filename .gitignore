.cache/
.cache/docling/
*/.cache/

# Ollama model cache
.ollama/
ollama/

# PyTorch cache
.torch/
torch_cache/

# HuggingFace cache
.huggingface/
transformers_cache/

# MIOpen cache (GPU)
miopen_cache/
/tmp/miopen_cache*

